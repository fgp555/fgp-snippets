{
  "Node Modules": {
    "prefix": "node_modules",
    "body": ["node_modules/"],
    "description": "Ignore node_modules directory"
  },
  "Logs": {
    "prefix": "logs",
    "body": ["*.log"],
    "description": "Ignore all log files"
  },
  "gitignore": {
    "prefix": "fgp_gitignore",
    "body": [
      "node_modules",
      "*.config",
      "*.jpg",
      "*.webp",
      "*.png",
      "*.svg",
      "*.rar",
      "*.zip",
      "*.bat",
      "*.sh",
      ".vite",
      "/img",
      "/lib",
      "/backup",
      "/.vscode",
      "/temp",
      "prepros.config",
      "dev.html",
      "*dev.*"
    ]
  }
}
