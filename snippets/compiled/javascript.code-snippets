      "  } catch (error) {",
      "    res.status(500).json({ error: error.message });",
      "  }",
      "});",
      "",
      "// Start server",
      "app.listen(PORT, () => {",
      "  console.log(`API running on http://localhost:\\${PORT}`);",
      "});"
