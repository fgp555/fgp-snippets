      "  try {",
      "    const [result] = await pool.query(",
      "      \"UPDATE users SET name = ?, email = ? WHERE id = ?\",",
      "      [name, email, id]",
      "    );",
      "",
      "    if (result.affectedRows === 0) {",
      "      return res.status(404).json({ message: \"User not found\" });",
      "    }",
      "",
      "    res.json({ id, name, email });",
      "  } catch (error) {",
      "    res.status(500).json({ error: error.message });",
      "  }",
      "});",
      "",
      "/*",
      "|--------------------------------------------------------------------------",
