      "  const { id } = req.params;",
      "",
      "  try {",
      "    const [rows] = await pool.query(",
      "      \"SELECT * FROM users WHERE id = ?\",",
      "      [id]",
      "    );",
      "",
      "    if (rows.length === 0) {",
      "      return res.status(404).json({ message: \"User not found\" });",
      "    }",
      "",
      "    res.json(rows[0]);",
