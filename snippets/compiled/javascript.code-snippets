      "/*",
      "|--------------------------------------------------------------------------",
      "| READ ALL - GET /users",
      "|--------------------------------------------------------------------------",
      "*/",
      "app.get(\"/users\", async (req, res) => {",
      "  try {",
      "    const [rows] = await pool.query(\"SELECT * FROM users\");",
      "    res.json(rows);",
      "  } catch (error) {",
      "    res.status(500).json({ error: error.message });",
      "  }",
      "});",
      "",
